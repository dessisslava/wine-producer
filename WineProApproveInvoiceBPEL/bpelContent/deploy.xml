<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03" xmlns:WineProWsWarehouseInventoryReserve="http://imse.uoc.gr/WineProWsWarehouseInventoryReserve/" xmlns:imse.uoc.gr="http://imse.uoc.gr">
  <process name="imse.uoc.gr:ApproveInvoice">
    <active>true</active>
    <retired>false</retired>
    <process-events generate="all"/>
    <provide partnerLink="client">
      <service name="imse.uoc.gr:ApproveInvoiceService" port="ApproveInvoicePort"/>
    </provide>
    <invoke partnerLink="WarehouseReservationPL">
      <service name="WineProWsWarehouseInventoryReserve:WineProWsWarehouseInventoryReserve" port="WineProWsWarehouseInventoryReserveSOAP"/>
    </invoke>
  </process>
</deploy>