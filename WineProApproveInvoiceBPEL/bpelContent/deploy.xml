<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03" xmlns:WineProWsWarehouseInventoryReserve="http://imse.uoc.gr/WineProWsWarehouseInventoryReserve/" xmlns:imse.uoc.gr="http://imse.uoc.gr" xmlns:winePriceWSDLFile="http://imse.uoc.gr/winePriceWSDLFile/">
  <process name="imse.uoc.gr:WineProApproveInvoiceBPEL">
    <active>true</active>
    <retired>false</retired>
    <process-events generate="all"/>
    <provide partnerLink="client">
      <service name="imse.uoc.gr:WineProApproveInvoiceBPELService" port="WineProApproveInvoiceBPELPort"/>
    </provide>
    <invoke partnerLink="ReservationPL">
      <service name="WineProWsWarehouseInventoryReserve:WineProWsWarehouseInventoryReserve" port="WineProWsWarehouseInventoryReserveSOAP"/>
    </invoke>
    <invoke partnerLink="PricePL">
      <service name="winePriceWSDLFile:winePriceWSDLFile" port="winePriceWSDLFileSOAP"/>
    </invoke>
  </process>
</deploy>