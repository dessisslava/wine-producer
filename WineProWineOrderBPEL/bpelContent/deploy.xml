<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03" xmlns:WineProWsWarehouseInventoryCheck="http://imse.uoc.gr/WineProWsWarehouseInventoryCheck/" xmlns:imse.uoc.gr="http://imse.uoc.gr">
  <process name="imse.uoc.gr:WineOrder">
    <active>true</active>
    <retired>false</retired>
    <process-events generate="all"/>
    <provide partnerLink="client">
      <service name="imse.uoc.gr:WineOrderService" port="WineOrderPort"/>
    </provide>
    <invoke partnerLink="WarehouseInventoryCheck">
      <service name="WineProWsWarehouseInventoryCheck:WineProWsWarehouseInventoryCheck" port="WineProWsWarehouseInventoryCheckSOAP"/>
    </invoke>
  </process>
</deploy>