<service name="WineProWineListWs">
	<description>
		Wine Pro Order WS
	</description>
	<parameter name="ServiceObjectSupplier">org.apache.axis2.extensions.spring.receivers.SpringServletContextObjectSupplier
	</parameter>
	<parameter name="SpringBeanName">springAwareService</parameter>
	<operation name="listAllWinesInBetween">
		<messageReceiver mep="http://www.w3.org/2004/08/wsdl/in-out" class="org.apache.axis2.rpc.receivers.RPCMessageReceiver" />
	</operation>
	<operation name="listAllWines">
		<messageReceiver mep="http://www.w3.org/2004/08/wsdl/in-out" class="org.apache.axis2.rpc.receivers.RPCMessageReceiver" />
	</operation>
	<excludeOperations>
		<operation>setWineService</operation>
	</excludeOperations>
</service> 